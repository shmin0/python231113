import re

# 정규표현식 패턴
pattern = r'\d{6}-(1|2)\d{6}'

# 주민등록번호 샘플 데이터
samples = [
    '900101-1234567',  # 유효한 예시
    '950512-2345678',  # 유효한 예시
    '871212-1567890',  # 유효한 예시
    '030304-2890123',  # 유효한 예시
    '751230-3567890',  # 유효한 예시
    '980215-15678',    # 잘못된 예시 (뒷부분 자릿수 부족)
    '880101-12345',    # 잘못된 예시 (뒷부분 자릿수 부족)
    '123456-1234567',  # 잘못된 예시 (앞부분 자릿수 부족)
    '990101-3567890',  # 유효한 예시
    '071212-1234abc',  # 잘못된 예시 (앞부분이 숫자가 아님)
]

# 주민등록번호 검사
for sample in samples:
    if re.match(pattern, sample):
        print(f"{sample}: 유효한 주민등록번호")
    else:
        print(f"{sample}: 잘못된 주민등록번호")